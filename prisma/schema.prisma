generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id() @default(uuid())
  email    String @unique
  name     String @unique
  pixKey   String @unique
  balance  Float @default(1000)
  password String
}
